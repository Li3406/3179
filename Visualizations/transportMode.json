{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {"text": "Mode of Transport"},
    "width": 500,
    "height": 250,
    "data": {
      "url": "https://raw.githubusercontent.com/Li3406/3179/main/Data/Extracted%20Data/transportMode.csv"
    },
    "mark": {"type": "arc", "innerRadius": 50},
    "encoding": {
      "theta": {"field": "visitor_percent", "type": "quantitative"},
      "color": {
        "field": "transport_mode",
        "type": "nominal",
        "scale": {"scheme": "tableau20"}
      },
      "tooltip": [
        {"field": "transport_mode", "type": "nominal", "title": "Mode:"},
        {
          "field": "visitor_percent",
          "type": "quantitative",
          "title": "Percentage"
        }
      ]
    }
  }